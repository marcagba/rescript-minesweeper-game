    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react-refresh": "^1.1.0",
    "bs-platform": "9.0.1",
    "npm-run-all": "^4.1.5",
    "vite": "^2.0.0-beta.12"
  }
}
